<?xml version="1.0" encoding="UTF-8"?>
<beans
xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="Student" class="com.learning.main.DemoSpring.Student">
<property name="id" value="1001"></property>
<property name="name" value="ajayreddy"></property>
<property name="address" value="hyderabad"></property>
</bean>
<bean id="Employee" class="com.learning.main.DemoSpring.Employee">
<property name="id" value="2000"></property>
<property name="age" value="23"></property>
<property name="name" value="suraj"></property>
</bean>
</beans>